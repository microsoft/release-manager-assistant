{
  "mimeTypes": {
    ".js": "text/javascript",
    ".mjs": "text/javascript",
    ".jsx": "text/javascript"
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}
